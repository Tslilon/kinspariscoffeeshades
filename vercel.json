{

  "functions": {
    "app/api/sunscore/route.ts": {
      "maxDuration": 30
    },
    "app/api/cafes/route.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NEXT_PUBLIC_MAPTILER_KEY": "@next_public_maptiler_key"
  },
  "headers": [
    {
      "source": "/vox/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, immutable"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/vox/(.*)",
      "destination": "/vox/$1"
    }
  ]
}
